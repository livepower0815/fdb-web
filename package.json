{
  "name": "fdb-nuxt-web",
  "version": "1.0.0",
  "engines": {
    "node": "14.17.0"
  },
  "private": true,
  "scripts": {
    "dev": "cross-env BASE=local NUXT_ENV_CURRENT_GIT_SHA=`git rev-parse --short HEAD` nuxt",
    "build:dev": "cross-env BASE=dev NUXT_ENV_CURRENT_GIT_SHA=`git rev-parse --short HEAD` nuxt build",
    "build:prod": "cross-env BASE=prod NUXT_ENV_CURRENT_GIT_SHA=`git rev-parse --short HEAD` nuxt build",
    "start": "node ./node_modules/nuxt/bin/nuxt.js start",
    "generate:dev": "cross-env BASE=dev NUXT_ENV_CURRENT_GIT_SHA=`git rev-parse --short HEAD` nuxt generate",
    "generate:prod": "cross-env BASE=prod NUXT_ENV_CURRENT_GIT_SHA=`git rev-parse --short HEAD` nuxt generate",
    "start:pm2": "pm2 start",
    "restart:pm2": "pm2 restart all",
    "stop:pm2": "pm2 stop all",
    "i18n-generator": "i18n ./locales/fdb_web.csv ./locales",
    "lint": "eslint **/*.{js,vue} --quiet",
    "style:css": "stylelint **/*.css --fix",
    "style:scss": "stylelint style/**/*.scss --fix",
    "style:vue": "stylelint pages/**/*.vue components/**/*.vue layouts/**/*.vue  --fix"
  },
  "dependencies": {
    "@gtm-support/vue2-gtm": "^1.2.1",
    "@nuxtjs/dotenv": "^1.4.1",
    "axios": "^0.21.1",
    "core-js": "^3.19.3",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.2",
    "echarts": "^4.9.0",
    "element-ui": "^2.15.6",
    "express": "^4.18.1",
    "i18n-generator": "^0.5.1",
    "nuxt": "^2.15.8",
    "sass": "1.32.*",
    "sass-loader": "^10.1.1",
    "vue": "^2.6.14",
    "vue-echarts": "^5.0.0-beta.0",
    "vue-i18n": "^8.22.4",
    "vue-server-renderer": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/eslint-parser": "^7.19.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "eslint": "^8.25.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.6.0",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.5",
    "prettier": "^2.7.1",
    "stylelint": "^14.13.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-prettier": "^2.0.0"
  }
}
